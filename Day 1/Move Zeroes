class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n = nums.size();
        int pos=-1;
        int cur=0;
        while(cur<n){
            if(nums[cur]!=0){
                pos++;
                swap(nums[cur],nums[pos]);
            }
            
        
        cur++;
        }
        
    }
};
